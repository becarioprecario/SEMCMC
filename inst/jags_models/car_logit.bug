model {

  #Response
  for(i in 1:N) {
    y[i] ~ dbern(p[i])
    logit(p[i]) <- y.latent[i]
  }
  y.latent[1:N] ~ dmnorm(mu[1:N,1], PREC[,])

  #Linear prediction term
  mu[1:N,1] <- X[,] %*% b[,]

  #I - lambda * W
  IlambdaW[1:N, 1:N] <- I[,] - lambda * W[,]
  
  #Build precision matrix for CAR error term
  PREC[1:N, 1:N] <-  IlambdaW

  for(i in 1:nvar) {
    b[i,1] ~ dnorm(0, 0.001)
  }

  #tau ~ dgamma(.01, .01)
  lambda ~  dunif(lambda_min, lambda_max)

}
